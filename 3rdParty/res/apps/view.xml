<AppProperties id="view">
    <CopyProperties url="res/apps/default.xml" />
    
    <StartMode>materialView</StartMode>
    

    <DisableMode>sculpt</DisableMode>
    <DisableMode>paint</DisableMode>
    <DisableMode>engrave</DisableMode>
    <DisableMode>reform</DisableMode>
    <DisableMode>deform</DisableMode>
	
    <Background type="gradient">
        <color0>0.90196 0.90196 0.90196 1</color0>
        <color1>0.90196 0.90196 0.90196 1</color1>
        <color2>1 1 1 1</color2>
        <color3>1 1 1 1</color3>
    </Background>
    
    <Gui url="res/gui/viewUI.xml" />
    <GuiCommands url="res/gui/GuiCommands_new.xml" />

    <StartCommands>
        <AppCmd cmd="SetRenderStyle" value="autoSmooth" />
        <AppCmd cmd="SetSurfaceApprox" value="0" />
    </StartCommands>
    

    
    <UnlimitedUpload />
	
    
</AppProperties>
