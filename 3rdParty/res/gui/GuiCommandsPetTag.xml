<GuiCommands>

<!--    <GuiCmd component="uploadButton" event="clicked" command="UploadObject"></GuiCmd> -->
	<GuiCmd component="uploadButton" event="clicked" command="OpenSEPSideBar" />
	
    <GuiCmd component="makerbotButton" event="clicked" command="UploadObject" value="makerbot"></GuiCmd>

    <GuiCmd component="undoButton" event="clicked" command="Undo"></GuiCmd>
    <GuiCmd component="redoButton" event="clicked" command="Redo"></GuiCmd>
    
    <GuiCmd component="templateSelectorButton" event="clicked" command="OpenTemplateSelector"/>

    <GuiCmd component="paintButton" event="clicked">
        <Command cmd="SetGuiCommandsOnlyMode"></Command>
    </GuiCmd>
    <GuiCmd component="rotateButton" event="clicked" command="ToggleObjectAutoRotMode"></GuiCmd>
    
    <GuiCmd component="engraveEmbossButton" event="clicked">
        <Command cmd="ResetCameraClosestView"></Command>
        <Command cmd="SetEngraveMode"></Command>
    </GuiCmd>
    
    <GuiCmd component="textInputButton" event="clicked">
        <Command cmd="ResetCameraClosestView"></Command>
        <Command cmd="OpenEngraveTextEditor"></Command>
    </GuiCmd>

    <GuiCmd component="rightArrowButton" event="clicked" command="ScrollPatternsLeft"></GuiCmd>
    <GuiCmd component="leftArrowButton" event="clicked" command="ScrollPatternsRight"></GuiCmd>
    
	<GuiCmd component="stampDepthSlider" event="sliderValueChanged" command="SetStampDepth" value="sliderValue"></GuiCmd>
	<GuiCmd component="stampDepthSlider" event="sliderReleased" command="SetStampDepth" value="sliderValue"></GuiCmd>
    
    <GuiCmd component="toolSizeSlider" event="sliderValueChanged" command="SetBrushSize" value="sliderValue"></GuiCmd>
    <GuiCmd component="toolSizeSlider.sliderKnob" event="pressed" command="ShowBrushRing" value="true"></GuiCmd>
    <GuiCmd component="toolSizeSlider.sliderKnob" event="released" command="ShowBrushRing" value="false"></GuiCmd>
    
    <GuiCmd component="redButton" event="clicked" command="PaintEntireMesh" value="1 0 0 1"></GuiCmd>
    <GuiCmd component="whiteButton" event="clicked" command="PaintEntireMesh" value="1 1 1 1"></GuiCmd>
    <GuiCmd component="yellowButton" event="clicked" command="PaintEntireMesh" value="1 1 0 1"></GuiCmd>
    <GuiCmd component="orangeButton" event="clicked" command="PaintEntireMesh" value="1 0.5255 0 1"></GuiCmd>
    <GuiCmd component="redButton" event="clicked" command="PaintEntireMesh" value="0.8157 0 0 1"></GuiCmd>
    <GuiCmd component="greenButton" event="clicked" command="PaintEntireMesh" value="0 0.7608 0 1"></GuiCmd>
    <GuiCmd component="lightBlueButton" event="clicked" command="PaintEntireMesh" value="0 0.6706 1 1"></GuiCmd>
    <GuiCmd component="darkBlueButton" event="clicked" command="PaintEntireMesh" value="0 0.1216 1 1"></GuiCmd>
    <GuiCmd component="pinkButton" event="clicked" command="PaintEntireMesh" value="1 0.4196 1 1"></GuiCmd>
    <GuiCmd component="purpleButton" event="clicked" command="PaintEntireMesh" value="0.4706 0 0.6941 1"></GuiCmd>
    <GuiCmd component="brownButton" event="clicked" command="PaintEntireMesh" value="0.5451 0.2784 0 1"></GuiCmd>
    <GuiCmd component="greyButton" event="clicked" command="PaintEntireMesh" value="0.5 0.5 0.5 1"></GuiCmd>
    <GuiCmd component="blackButton" event="clicked" command="PaintEntireMesh" value="0.1176 0.1176 0.1176 1"></GuiCmd>
    <GuiCmd component="colorIndicatorButton" event="clicked" command="PaintEntireMesh"></GuiCmd>
	
	<!--  text input, freehand custom pattern -->
    <GuiCmd component="textInputButton" event="clicked" command="OpenEngraveTextEditor" />
    <GuiCmd component="freeHandButton" event="clicked" command="OpenEngraveFreehand" />
    <GuiCmd component="customPatternButton" event="clicked" command="SelectPattern" value="custom" />
    <GuiCmd component="customPatternButton" event="releasedAfterSetOff" command="ImportFile" value="image" />	
</GuiCommands>
